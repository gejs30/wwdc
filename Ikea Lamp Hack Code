#Saturday 11 November 2023
import time

from adafruit_circuitplayground.express import cpx

while True:
    cpx.red_led = not cpx.red_led
    time.sleep(0.5)

#Sunday 11 November 2023
#Absolute Value Blink Neopixel 0
import time

from adafruit_circuitplayground.express import cpx

while True:
    cpx.pixels[0] = (abs(cpx.pixels[0][0]-255), 0, 0)
    time.sleep(0.5)
    
